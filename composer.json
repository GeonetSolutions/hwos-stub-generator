{
    "name": "sebastiaanluca/php-file-generator",
    "type": "library",
    "description": "Easily generate new files and classes from stubs.",
    "keywords": [
        "sebastiaanluca",
        "php-file-generator"
    ],
    "homepage": "https://github.com/sebastiaanluca/php-file-generator",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastiaan Luca",
            "email": "hello@sebastiaanluca.com",
            "homepage": "https://www.sebastiaanluca.com",
            "role": "Author"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^6"
    },
    "autoload": {
        "psr-4": {
            "SebastiaanLuca\\FileGenerator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SebastiaanLuca\\FileGenerator\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
